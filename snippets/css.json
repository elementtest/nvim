{
  "CSS Setup": {
    "prefix": "csssetup",
    "body": [
      "/* Google Fonts */",
      "@import url('https://fonts.googleapis.com/css2?family=$1&display=swap');",
      "",
      "/* Base styles */",
      "body {",
      "    font-family: '$1', sans-serif;",
      "    margin: 0;",
      "    padding: 0;",
      "    box-sizing: border-box;",
      "    background-color: $2;",
      "    color: $3;",
      "}",
      "",
      "/* Links */",
      "a {",
      "    color: $4;",
      "    text-decoration: none;",
      "}",
      "$0"
    ],
    "description": "Basic CSS setup with Google Fonts and base styles"
  }
}

